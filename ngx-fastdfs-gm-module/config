ngx_addon_name=ngx_http_fastdfs_gm_module
HTTP_MODULES="$HTTP_MODULES ngx_http_fastdfs_gm_module"
NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_fastdfs_gm_module.c"
CORE_INCS="$CORE_INCS /usr/local/include/fastdfs /usr/local/include/fastcommon/"
CORE_LIBS="$CORE_LIBS -L/usr/local/lib -lfastcommon -lfdfsclient"
CFLAGS="$CFLAGS -D_FILE_OFFSET_BITS=64 -DFDFS_OUTPUT_CHUNK_SIZE='256*1024' -DFDFS_GM_MOD_CONF_FILENAME='\"/etc/fdfs/mod_fastdfs_gm.conf\"'"

